{
  "Request": {
    "Path": "/translate/shakespeare",
    "Methods": [
      "POST"
    ],
    "Headers": [
      {
        "Name": "x-scenario",
        "Matchers": [
          {
            "Name": "WildcardMatcher",
            "Pattern": "GivenShakespeareTranslationType_WhenTextIsSubmitted_ThenItIsReturnedTranslated",
            "IgnoreCase": true
          }
        ]
      }
    ],
    "Body": {
      "Matcher": {
        "Name": "WildcardMatcher",
        "Pattern": "text=Master+Obiwan+has+lost+a+planet.",
        "IgnoreCase": true
      }
    }
  },
  "Response": {
    "StatusCode": 200,
    "BodyAsJson": {
      "success": {
        "total": 1
      },
      "contents": {
        "translated": "Master obiwan hath did lose a planet.",
        "text": "Master Obiwan has lost a planet.",
        "translation": "shakespeare"
      }
    },
    "BodyAsJsonIndented": true,
    "Headers": {
      "Server": [
        "nginx/1.14.0",
        "(Ubuntu)"
      ],
      "Connection": [
        "keep-alive"
      ],
      "Access-Control-Allow-Origin": [
        "*"
      ],
      "Vary": [
        "Accept"
      ],
      "X-Auth-Status": [
        "true"
      ],
      "X-RateLimit-Limit": [
        "10 per hour"
      ],
      "Cache-Control": [
        "max-age={expires}, pre-check=86400, post-check=43200"
      ],
      "X-Powered-By": [
        "Luracast Restler v3.0.0rc3"
      ],
      "Content-Type": [
        "application/json; charset=utf-8"
      ],
      "Content-Length": [
        "222"
      ],
      "Expires": [
        "0"
      ],
      "Content-Language": [
        "en"
      ]
    }
  }
}